        <dependency id="ZXing.Net" version="0.16.5" />
      </group>
      <group targetFramework=".NETStandard2.0">
        <dependency id="ThoughtWorks.QRCode.Core" version="1.0.1.2" />
        <dependency id="ZXing.Net" version="0.16.5" />
        <dependency id="ZXing.Net.Bindings.Windows.Compatibility" version="0.16.6" />
      </group>
    </dependencies>
  </metadata>
</package>